@rem ProudNetClient.dll 파일을 복사합니다.
set dll_path=

set folders=$(ConfigurationName)-x86_32 $(ConfigurationName)-x86_64 $(ConfigurationName)_static_CRT-x86_32 $(ConfigurationName)_static_CRT-x86_64
for %25%25f in (%25folders%25) do (
    set file_path=$(ProjectDir)..\..\core_cpp\ProudNetClientDll\bin\%25%25f\ProudNetClient.dll
    if exist !file_path! (
        set dll_path=!file_path!
    )
)

if "%25dll_path%25" == "" (
    echo #### ProudNetClient.dll 파일을 찾을 수 없습니다.
    exit
)

copy /y %25dll_path%25 $(TargetDir)


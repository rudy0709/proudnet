@rem PNLicenseAuth.exe 파일경로를 찾습니다.
set pnlicenseauth_path=

set folders=$(ConfigurationName)-x86_32 $(ConfigurationName)-x86_64 $(ConfigurationName)_static_CRT-x86_32 $(ConfigurationName)_static_CRT-x86_64
for %25%25f in (%25folders%25) do (
    set file_path=$(ProjectDir)..\PNLicenseAuth\bin\%25%25f\PNLicenseAuth.exe
    if exist !file_path! (
        set pnlicenseauth_path=!file_path!
    )
)

if "%25pnlicenseauth_path%25" == "" (
    echo #### PNLicenseAuth.exe 파일을 찾을 수 없습니다.
    echo #### PNLicenseAuth 프로젝트들을 먼저 빌드해야 합니다.
    exit
)

@rem .exe 파일에 디지털 서명을 추가합니다.
copy /y $(TargetPath) $(RootNameSpace)$(TargetExt)
"%25PN_SIGN_TOOL_PATH%25" sign /f $(ProjectDir)..\..\utils\CodeSign\202108_proudDigital.pfx /t http://timestamp.comodoca.com/authenticode /p OHOSZN /v $(TargetDir)$(RootNameSpace)$(TargetExt)

@rem PNLicenseAuthGui.exe 실행 시에 필요한 PNLicenseAuth.exe 파일을 복사합니다.
copy /y "%25pnlicenseauth_path%25" $(TargetDir)

@rem 두 실행파일을 공용 폴더로 복사합니다.
set util_folder=$(ProjectDir)..\..\utils\ProudNet
if not exist "%25util_folder%25" (
    mkdir "%25util_folder%25"
)

copy /y $(TargetDir)PNLicenseAuthGui.exe "%25util_folder%25"
copy /y $(TargetDir)PNLicenseAuth.exe "%25util_folder%25"</PostBuildEvent>
